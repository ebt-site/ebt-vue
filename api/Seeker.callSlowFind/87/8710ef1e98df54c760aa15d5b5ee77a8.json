        "segMap": {
          "an3.63:5.3": {
            "scid": "an3.63:5.3",
            "pli": "Dibbaṁ uccāsayanamahāsayanaṁ, brahmaṁ uccāsayanamahāsayanaṁ, ariyaṁ uccāsayanamahāsayanaṁ. ",
            "en": "The <span class=\"ebt-matched\">heavenly high and luxurious bed</span>, the divine high and luxurious bed, and the noble high and luxurious bed. ",
            "matched": true
          },
          "an3.63:6.1": {
            "scid": "an3.63:6.1",
            "pli": "“Katamaṁ pana taṁ, bho gotama, dibbaṁ uccāsayanamahāsayanaṁ, yassa bhavaṁ gotamo etarahi nikāmalābhī akicchalābhī akasiralābhī”ti? ",
            "en": "“But what, Master Gotama, is the <span class=\"ebt-matched\">heavenly high and luxurious bed</span>?” ",
            "matched": true
          },
          "an3.63:6.13": {
            "scid": "an3.63:6.13",
            "pli": "Idaṁ kho, brāhmaṇa, dibbaṁ uccāsayanamahāsayanaṁ, yassāhaṁ etarahi nikāmalābhī akicchalābhī akasiralābhī”ti. ",
            "en": "This is the <span class=\"ebt-matched\">heavenly high and luxurious bed</span> that I get these days when I want, without trouble or difficulty.” ",
            "matched": true
          },
          "an3.63:7.2": {
            "scid": "an3.63:7.2",
            "pli": "Ko cañño evarūpassa dibbassa uccāsayanamahāsayanassa nikāmalābhī bhavissati akicchalābhī akasiralābhī, aññatra bhotā gotamena. ",
            "en": "Who but Master Gotama could get such a <span class=\"ebt-matched\">heavenly high and luxurious bed</span> when he wants, without trouble or difficulty? ",
            "matched": true
          }
