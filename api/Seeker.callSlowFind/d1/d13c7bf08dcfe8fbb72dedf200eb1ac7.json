        "segMap": {
          "sn7.12:1.6": {
            "scid": "sn7.12:1.6",
            "pli": "“pakaṭṭhakoyaṁ samaṇo gotamo punappunaṁ āgacchatī”ti. ",
            "de": "„Dieser unersättliche Asket Gotama kommt <span class=\"ebt-matched\">wieder und wieder</span> her!“ ",
            "en": "“This insatiable ascetic Gotama keeps coming back again and again!” ",
            "matched": true
          },
          "sn7.12:2.1": {
            "scid": "sn7.12:2.1",
            "pli": "“Punappunañceva vapanti bījaṁ, ",
            "de": "„<span class=\"ebt-matched\">Wieder und wieder</span> säen sie die Saat; ",
            "en": "“Again and again, they sow the seed; ",
            "matched": true
          },
          "sn7.12:2.2": {
            "scid": "sn7.12:2.2",
            "pli": "Punappunaṁ vassati devarājā; ",
            "de": "<span class=\"ebt-matched\">wieder und wieder</span> schickt Gott der Herr Regen; ",
            "en": "again and again, the lord god sends rain; ",
            "matched": true
          },
          "sn7.12:2.3": {
            "scid": "sn7.12:2.3",
            "pli": "Punappunaṁ khettaṁ kasanti kassakā, ",
            "de": "<span class=\"ebt-matched\">wieder und wieder</span> pflügen Bauern das Feld; ",
            "en": "again and again, farmers plough the field; ",
            "matched": true
          },
          "sn7.12:2.4": {
            "scid": "sn7.12:2.4",
            "pli": "Punappunaṁ dhaññamupeti raṭṭhaṁ. ",
            "de": "<span class=\"ebt-matched\">wieder und wieder</span> wird das Korn für das Land erzeugt. ",
            "en": "again and again, grain is produced for the nation. ",
            "matched": true
          },
          "sn7.12:3.1": {
            "scid": "sn7.12:3.1",
            "pli": "Punappunaṁ yācakā yācayanti, ",
            "de": "<span class=\"ebt-matched\">Wieder und wieder</span> betteln die Bettler; ",
            "en": "Again and again, the beggars beg; ",
            "matched": true
          },
          "sn7.12:3.2": {
            "scid": "sn7.12:3.2",
            "pli": "Punappunaṁ dānapatī dadanti; ",
            "de": "<span class=\"ebt-matched\">wieder und wieder</span> geben die Spender; ",
            "en": "again and again, the donors give. ",
            "matched": true
          },
          "sn7.12:3.3": {
            "scid": "sn7.12:3.3",
            "pli": "Punappunaṁ dānapatī daditvā, ",
            "de": "wenn die Spender <span class=\"ebt-matched\">wieder und wieder</span> gegeben haben, ",
            "en": "Again and again, when the donors have given, ",
            "matched": true
          },
          "sn7.12:3.4": {
            "scid": "sn7.12:3.4",
            "pli": "Punappunaṁ saggamupenti ṭhānaṁ. ",
            "de": "nehmen sie <span class=\"ebt-matched\">wieder und wieder</span> ihren Platz im Himmel ein. ",
            "en": "again and again, they take their place in heaven. ",
            "matched": true
          },
          "sn7.12:4.1": {
            "scid": "sn7.12:4.1",
            "pli": "Punappunaṁ khīranikā duhanti, ",
            "de": "<span class=\"ebt-matched\">Wieder und wieder</span> melken die Milchbauern; ",
            "en": "Again and again, dairy farmers milk; ",
            "matched": true
          },
          "sn7.12:4.2": {
            "scid": "sn7.12:4.2",
            "pli": "Punappunaṁ vaccho upeti mātaraṁ; ",
            "de": "<span class=\"ebt-matched\">wieder und wieder</span> geht das Kalb zu seiner Mutter; ",
            "en": "again and again, a calf cleaves to its mother; ",
            "matched": true
          },
          "sn7.12:4.3": {
            "scid": "sn7.12:4.3",
            "pli": "Punappunaṁ kilamati phandati ca, ",
            "de": "<span class=\"ebt-matched\">wieder und wieder</span> unterdrückt der Schwachkopf und schüchtert ein; ",
            "en": "again and again, oppressing and intimidating; ",
            "matched": true
          },
          "sn7.12:4.4": {
            "scid": "sn7.12:4.4",
            "pli": "Punappunaṁ gabbhamupeti mando. ",
            "de": "<span class=\"ebt-matched\">wieder und wieder</span> wird er wiedergeboren. ",
            "en": "that idiot is reborn again and again. ",
            "matched": true
          },
          "sn7.12:5.1": {
            "scid": "sn7.12:5.1",
            "pli": "Punappunaṁ jāyati mīyati ca, ",
            "de": "<span class=\"ebt-matched\">Wieder und wieder</span> wirst du wiedergeboren und stirbst; ",
            "en": "Again and again, you’re reborn and die; ",
            "matched": true
          },
          "sn7.12:5.2": {
            "scid": "sn7.12:5.2",
            "pli": "Punappunaṁ sivathikaṁ haranti; ",
            "de": "<span class=\"ebt-matched\">wieder und wieder</span> wirst du zum Leichenfeld getragen. ",
            "en": "again and again, you get carried to a charnel ground. ",
            "matched": true
          },
          "sn7.12:5.4": {
            "scid": "sn7.12:5.4",
            "pli": "Na punappunaṁ jāyati bhūripañño”ti. ",
            "de": "wird einer von ausgedehnter Weisheit nicht <span class=\"ebt-matched\">wieder und wieder</span> geboren.“ ",
            "en": "one of vast wisdom is not reborn again and again.” ",
            "matched": true
          }
        },
